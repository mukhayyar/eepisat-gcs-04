<Window x:Class="GCS_EEPISAT_04.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GCS_EEPISAT_04"
        xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
        xmlns:gvm="clr-namespace:GraphGCS04.UsersControl"
        xmlns:gmapwf="clr-namespace:GMap.NET.WindowsForms;assembly=GMap.NET.WindowsForms"
        xmlns:h="http://helix-toolkit.org/wpf"
        mc:Ignorable="d"
        Background="#161616"
        WindowState="Maximized"
        WindowStyle="None"
        Loaded="WindowLoaded"
        Title="GCS 04" Height="768" Width="1366">
    <Border Background="#161616">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="280" />
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <!--NavPanel-->
            <Grid>
                <Border Grid.Row="1" CornerRadius="30" Background="#222222" Margin="15 15 15 15">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="270"></RowDefinition>
                            <RowDefinition Height="10"></RowDefinition>
                            <RowDefinition Height="310"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0" Background="#292929" CornerRadius="30 30 30 30" Margin="20 20 20 20"/>
                        <Grid Grid.Row="0" Margin="20 20 20 20">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0.7*"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Source="/Assets/Team.png" Height="30"></Image>
                                <Label x:Name="TeamIDLabel" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20" FontWeight="Bold" Foreground="White" Content="1085"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0.7*"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Source="/Assets/Time.png" Height="53"></Image>
                                <Grid Grid.Column="1">
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Label Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Content="MISSION TIME" FontSize="15" FontWeight="Bold" Foreground="White"/>
                                    <Label x:Name="MissionTimeLabel" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Top" Content="00:00:00.00" FontSize="20" FontWeight="Regular" Foreground="White"/>
                                </Grid>
                            </Grid>
                            <Grid Grid.Row="2">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0.7*"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Source="/Assets/Status Meter.png" Height="35" Width="58"></Image>
                                <TextBlock x:Name="SoftwareStateLabel" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="BOOT" FontSize="14" FontWeight="Bold" Foreground="White" TextWrapping="Wrap" TextAlignment="Center"/>
                            </Grid>
                        </Grid>
                        <Border  Grid.Row="1" Background="#FFFFFF" Width="250" Height="5" CornerRadius="2" />
                        <Grid Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Margin="0 15 0 0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="60" />
                                <RowDefinition Height="60" />
                                <RowDefinition Height="60" />
                                <RowDefinition Height="60" />
                                <RowDefinition Height="60" />
                            </Grid.RowDefinitions>
                            <RadioButton x:Name="HomeMenu" Grid.Row="0" Click="homeNavClick" Style="{DynamicResource HomeMenu}" IsChecked="True"/>
                            <RadioButton x:Name="GraphMenu" Grid.Row="1" Click="graphNavClick" Style="{DynamicResource GraphMenu}" />
                            <RadioButton x:Name="MapMenu" Grid.Row="2" Click="mapNavClick" Style="{DynamicResource MapMenu}" />
                            <RadioButton x:Name="DataMenu" Grid.Row="3" Click="dataCsvNavClick" Style="{DynamicResource DataMenu}" />
                            <RadioButton x:Name="SettingMenu" Grid.Row="4" Click="settingNavClick" Style="{DynamicResource SettingMenu}" />
                        </Grid>
                        <Viewbox Grid.Row="3" Width="80" Height="100" Margin="0 0 0 40">
                            <StackPanel Orientation="Horizontal">
                                <Border BorderBrush="#00c000" BorderThickness="2" CornerRadius="5" Padding="2" Width="100" Height="50">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Border x:Name="batteryPercentage" Width="70" Grid.Column="0" Background="#00c000" CornerRadius="2" Padding="2" />
                                    </Grid>
                                </Border>
                                <Border Background="#00c000" CornerRadius="2,2,2,2" Padding="2" Width="7" Height="20" Margin="0,-2,0,-2"/>
                            </StackPanel>
                        </Viewbox>
                        <Label x:Name="lblBatteryStatus" Grid.Row="3" Content="70%" Foreground="#FFFFFF" FontWeight="Bold" FontSize="14" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0 0 0 10"/>
                    </Grid>
                </Border>
            </Grid>
            <Grid Grid.Column="1">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="150"></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <Border Grid.Row="0" CornerRadius="30" Background="#222222" Margin="15 15 15 10">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition Width="170"></ColumnDefinition>
                                <ColumnDefinition Width="110"></ColumnDefinition>
                                <ColumnDefinition Width="110"></ColumnDefinition>
                                <ColumnDefinition Width="20"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" CornerRadius="20" Background="#292929" Margin="15 10 15 10">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="270"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Border Grid.Column="0" CornerRadius="20" Background="#F1F1F1">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="90"></ColumnDefinition>
                                                <ColumnDefinition Width="90"></ColumnDefinition>
                                                <ColumnDefinition Width="90"></ColumnDefinition>
                                            </Grid.ColumnDefinitions>
                                            <Image Grid.Column="0" Source="/Assets/CanSat.png" Style="{DynamicResource Icon}" />
                                            <Image Grid.Column="1" Source="/Assets/pens.png" Style="{DynamicResource Icon}" />
                                            <Image Grid.Column="2" Source="/Assets/EEPISat.png" Style="{DynamicResource Icon}" />
                                        </Grid>
                                    </Border>
                                    <Label Grid.Column="1" Margin="0 25 0 0" Content="BAMANTARA EEPISAT | GROUND CONTROL STATION" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Top" FontWeight="Bold" FontSize="13"/>
                                    <Label Grid.Column="1" Margin="0 0 0 25" Content="POLITEKNIK ELEKTRONIKA NEGERI SURABAYA" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontWeight="Bold" FontSize="14"/>
                                </Grid>
                            </Border>
                            <Border Grid.Column="1" CornerRadius="30" Background="#292929" Margin="5 10 5 10">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="5"></ColumnDefinition>
                                        <ColumnDefinition Width="50"></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="5"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Image Grid.Column="1" Source="/Assets/Flag.png" Style="{DynamicResource IconItemList}" />
                                    <Label Grid.Column="2" Content="INDONESIA" Foreground="#FFFFFF" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="16"/>
                                </Grid>
                            </Border>
                            <Border Grid.Column="2" CornerRadius="30" Background="#292929" Margin="5 10 5 10">
                                <Button x:Name="RestartBtn" Click="restartBtnClick" Style="{DynamicResource ResetBtn}"/>
                            </Border>
                            <Border Grid.Column="3" CornerRadius="30" Background="#292929" Margin="5 10 5 10">
                                <Button x:Name="ShutdownBtn" Click="shutdownBtnClick" Style="{DynamicResource ShutdownBtn}"/>
                            </Border>
                        </Grid>
                    </Border>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition Width="230"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Border Grid.Column="0" CornerRadius="30" Background="#222222" Margin="15 10 15 15">
                            <TabControl x:Name="MainPage" Background="Transparent" BorderBrush="Transparent" ScrollViewer.CanContentScroll="True" SelectionChanged="MainPage_SelectionChanged">
                                <TabControl.ItemContainerStyle>
                                    <Style TargetType="{x:Type TabItem}">
                                        <Setter Property="Visibility" Value="Collapsed"/>
                                    </Style>
                                </TabControl.ItemContainerStyle>
                                <TabItem>
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*"></ColumnDefinition>
                                            <ColumnDefinition Width="1*"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="1.5*"></RowDefinition>
                                                <RowDefinition Height="0.95*"></RowDefinition>
                                                <RowDefinition Height="0.95*"></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Grid Grid.Row="0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition></ColumnDefinition>
                                                    <ColumnDefinition></ColumnDefinition>
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.Column="0">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition></RowDefinition>
                                                        <RowDefinition Height="0.7*"></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Border Grid.Row="0" CornerRadius="30" Background="#292929" Margin="15 15 15 5">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="55"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Image Grid.Row="0" Margin="0 10 0 0" Source="/Assets/GPSNew.png" Style="{DynamicResource IconItemList}" VerticalAlignment="Top" />
                                                            <Label Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Center" Content="GPS TIME" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF"/>
                                                            <Label x:Name="GPSTimeLabel" Grid.Row="1" Margin="10 15 10 10" VerticalAlignment="Top" HorizontalAlignment="Center" Content="00:00:00" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF"/>
                                                            <Label Grid.Row="1" Margin="0 15 0 0" VerticalAlignment="Center" HorizontalAlignment="Center" Content="SATS" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF"/>
                                                            <Label x:Name="GPSCountLabel" Grid.Row="1" Margin="10 10 10 3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Content="00" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF"/>
                                                        </Grid>
                                                    </Border>
                                                    <Border Grid.Row="1" CornerRadius="30" Background="#292929" Margin="15 5 15 15">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="40"></RowDefinition>
                                                                <RowDefinition Height="40"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid Grid.Row="0">
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition></ColumnDefinition>
                                                                    <ColumnDefinition></ColumnDefinition>
                                                                </Grid.ColumnDefinitions>
                                                                <Label Grid.Column="0" VerticalAlignment="Top" HorizontalAlignment="Center" Content="LONGITUDE" FontWeight="Bold" FontSize="10" Foreground="#FFFFFF"/>
                                                                <Label x:Name="GPSLongitudeLabel" Grid.Column="0" VerticalAlignment="Bottom" HorizontalAlignment="Center" Content="0.0000" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF"/>
                                                                <Label Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Center" Content="LATITUDE" FontWeight="Bold" FontSize="10" Foreground="#FFFFFF"/>
                                                                <Label x:Name="GPSLatitudeLabel" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Content="0.0000" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF"/>
                                                            </Grid>
                                                            <Label Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Center" Content="ALTITUDE" FontWeight="Bold" FontSize="10" Foreground="#FFFFFF"/>
                                                            <Label x:Name="GPSAltitudeLabel" Grid.Row="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Content="0.0" Margin="0 0 20 0" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF"/>
                                                            <Label Grid.Row="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Content="m" Margin="35 0 0 0" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF"/>
                                                        </Grid>
                                                    </Border>
                                                </Grid>
                                                <Border Grid.Column="1" CornerRadius="30" Background="#292929" Margin="15 15 15 15">
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="90"></RowDefinition>
                                                            <RowDefinition Height="80"></RowDefinition>
                                                            <RowDefinition Height="30"></RowDefinition>
                                                            <RowDefinition Height="30"></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Grid Grid.Row="0">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Image Margin="20 10 10 10" Grid.Column="0" Source="/Assets/MaxAltitude.png" Style="{DynamicResource IconItemList}" HorizontalAlignment="Left" />
                                                            <Grid Grid.Column="1">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="1*"></RowDefinition>
                                                                    <RowDefinition Height="1*"></RowDefinition>
                                                                    <RowDefinition Height="1*"></RowDefinition>
                                                                    <RowDefinition Height="1*"></RowDefinition>
                                                                </Grid.RowDefinitions>
                                                                <Label Grid.Row="0" Margin="-40 0 0 -10" Content="PACKET COUNT" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label x:Name="PacketCountLabel" Grid.Row="1" Margin="0 -8 40 0" Content="0" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label Grid.Row="2" Margin="-40 -10 0 0" Content="ALTITUDE" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label x:Name="AltitudeLabel" Grid.Row="3" Margin="0 -20 40 0" Content="0.0" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label Grid.Row="3" Margin="0 -37 30 0" Content="m" VerticalAlignment="Center" HorizontalAlignment="Right" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                            </Grid>
                                                        </Grid>
                                                        <Grid Grid.Row="1">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Grid Grid.Column="0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                </Grid.RowDefinitions>
                                                                <Image Margin="0,-5,0,0" Grid.Row="0"  Source="/Assets/Voltage.png" Style="{DynamicResource IconItemList}" HorizontalAlignment="Center" />
                                                                <Label Grid.Row="1"  Content="VOLT" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label Margin="0, 15, 0, 0" x:Name="VoltageLabel" Grid.Row="1"  Content="0.0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label Grid.Row="1" Margin="0, 15, 10, 0" Content="V" VerticalAlignment="Center" HorizontalAlignment="Right" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                            </Grid>
                                                            <Grid Grid.Column="1">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                </Grid.RowDefinitions>
                                                                <Image Margin="0,-5,0,0" Grid.Row="0" Source="/Assets/Temperature.png" Style="{DynamicResource IconItemList}" HorizontalAlignment="Center" />
                                                                <Label Grid.Row="1" Content="TEMP" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label Margin="0, 15, 0, 0" x:Name="TemperatureLabel" Grid.Row="1" Content="0.0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                                <Label Grid.Row="1" Margin="0, 15, 10, 0" Content="°C" VerticalAlignment="Center" HorizontalAlignment="Right" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                            </Grid>
                                                        </Grid>
                                                        <Grid Grid.Row="2">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Label Grid.Column="1" Margin="0 -10 0 0" Content="X" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                            <Label Grid.Column="2" Margin="0 -10 40 0" Content="Y" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                        </Grid>
                                                        <Grid Grid.Row="2">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="50"></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Label Grid.Column="0" Margin="0 0 -40 0" Content="TILT" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                            <Label x:Name="TiltXLabel" Grid.Column="1" Margin="0 0 0 0" Content="0.0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                            <Label x:Name="TiltYLabel" Grid.Column="2" Margin="-40 0 0 0" Content="0.0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                        </Grid>
                                                        <Grid Grid.Row="3">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition></RowDefinition>
                                                                <RowDefinition></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Label Margin="0, -10, 0, 0" Grid.Row="0" Content="PRESSURE" VerticalAlignment="Top" HorizontalAlignment="Center" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                            <Label Margin="0, -15, 0, 0" x:Name="PressureLabel" Grid.Row="1" Content="0.0" VerticalAlignment="Center" HorizontalAlignment="Center" FontWeight="Regular" FontSize="12" Foreground="#FFFFFF" />
                                                            <Label Grid.Row="1" Margin="0, -15, 45, 0" Content="kPa" VerticalAlignment="Center" HorizontalAlignment="Right" FontWeight="Bold" FontSize="12" Foreground="#FFFFFF" />
                                                        </Grid>
                                                    </Grid>
                                                </Border>
                                            </Grid>
                                            <Border Grid.Row="1" CornerRadius="30" Background="#292929" Margin="15 5 15 5">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="0.45*"></RowDefinition>
                                                        <RowDefinition Height="2.2*"></RowDefinition>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="0.35*"></ColumnDefinition>
                                                            <ColumnDefinition></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Image Grid.Column="0" Source="/Assets/Command Prompt.png" Margin="0 -8 0 -10" Width="20" Style="{DynamicResource IconItemList}"/>
                                                        <Label Grid.Column="1" Content="COMMAND PROMPT" Margin="-30 -4 0 -10" Foreground="#FFFFFF" FontSize="15" FontWeight="Bold"/>
                                                    </Grid>
                                                    <Border Grid.Row="1" Margin="15 5 15 0" Background="White" CornerRadius="10">
                                                        <TextBox x:Name="CMDTextBox2" VerticalScrollBarVisibility="Auto" TextWrapping="Wrap" Margin="15 2 15 2" BorderThickness="0" IsReadOnly="True"></TextBox>
                                                    </Border>
                                                        <Grid Grid.Row="2">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="0.2*"></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                                <ColumnDefinition Width="0.2*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                        <Border Grid.Column="0" Margin="15,5,0,5" Background="White" CornerRadius="10">
                                                            <ComboBox x:Name="CMDShortcut" Background="Transparent" BorderThickness="0" Margin="5,5,5,5" SelectionChanged="CMDShortcutChanged">
                                                                <ComboBoxItem>CAL</ComboBoxItem>
                                                                <ComboBoxItem>PXON</ComboBoxItem>
                                                                <ComboBoxItem>PXOFF</ComboBoxItem>
                                                                <ComboBoxItem>ST</ComboBoxItem>
                                                                <ComboBoxItem>SIMENABLE</ComboBoxItem>
                                                                <ComboBoxItem>SIMDISABLE</ComboBoxItem>
                                                                <ComboBoxItem>SIMACTIVATE</ComboBoxItem>
                                                                <ComboBoxItem>SIMP</ComboBoxItem>
                                                                <ComboBoxItem>MECHANIC</ComboBoxItem>
                                                            </ComboBox>
                                                        </Border>
                                                        <Border Grid.Column="1" Margin="10 5 5 5" Background="White" CornerRadius="10">
                                                                <TextBox x:Name="CMDTextBox1" Margin="15 2 15 2" BorderThickness="0" KeyDown="CMDTextBox_KeyPress"></TextBox>
                                                            </Border>
                                                            <Border Grid.Column="2" Margin="5 5 15 5" Background="White" CornerRadius="10">
                                                            <Button x:Name="CMDSendButton" Click="CMDSendButtonClick" Background="White" Margin="5 2 5 2" BorderThickness="0">
                                                                <TextBlock>📡</TextBlock></Button>
                                                            </Border>
                                                        </Grid>
                                                </Grid>
                                            </Border>
                                            <Border Grid.Row="2" CornerRadius="30" Background="#292929" Margin="15 5 15 5">
                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="0.5*"></RowDefinition>
                                                        <RowDefinition Height="2.7*"></RowDefinition>
                                                        <RowDefinition></RowDefinition>
                                                    </Grid.RowDefinitions>
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="100"></ColumnDefinition>
                                                            <ColumnDefinition></ColumnDefinition>
                                                        </Grid.ColumnDefinitions>
                                                        <Image Grid.Column="0" Source="/Assets/Serial Control.png" Margin="0 -8 0 -10" Width="20" Style="{DynamicResource IconItemList}"/>
                                                        <Label Grid.Column="1" Content="SERIAL CONTROL" Margin="-30 -4 0 -10" Foreground="#FFFFFF" FontSize="15" FontWeight="Bold"/>
                                                    </Grid>
                                                    <Border Grid.Row="1" Margin="15 5 15 0" Background="White" CornerRadius="10">
                                                        <TextBox x:Name="SerialControlTextBox" Margin="15 2 15 2" BorderThickness="0" 
                                                            IsReadOnly="True" 
                                                            TextChanged="SerialControlTextBox_TextChanged"
                                                            TextWrapping="Wrap"
                                                            VerticalScrollBarVisibility="Auto"></TextBox>
                                                    </Border>
                                                    <Border x:Name="PortStatusPane" Background="#C12C2C" Grid.Row="2" Margin="15 5 15 5" CornerRadius="10">
                                                        <Label x:Name="PortStatus" Margin="15 -5 15 -5" HorizontalAlignment="Center" Content="Disconnected From Serial Port" Style="{DynamicResource NavMenuLabel}"/>
                                                    </Border>
                                                </Grid>
                                            </Border>

                                        </Grid>
                                        <Grid Grid.Column="1">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="0.8*"></RowDefinition>
                                                <RowDefinition Height="1.2*"></RowDefinition>
                                                <RowDefinition></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <Border Grid.Row="0" CornerRadius="30" Background="#292929" Margin="10 15 10 10">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="2.5*"></ColumnDefinition>
                                                        <ColumnDefinition></ColumnDefinition>
                                                        <ColumnDefinition></ColumnDefinition>
                                                        <ColumnDefinition></ColumnDefinition>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid Grid.Column="0">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="0.3*"></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Label Grid.Row="0" Content="PEAK PAYLOAD DATA Rx:" Foreground="LightGreen" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold" FontSize="12"/>
                                                        <Grid Grid.Row="1">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="1.3*"></ColumnDefinition>
                                                                <ColumnDefinition></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <Grid Grid.Column="0">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                    <RowDefinition></RowDefinition>
                                                                </Grid.RowDefinitions>
