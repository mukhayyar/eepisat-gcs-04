                                Foreground="White" />
                            <CheckBox
                                x:Name="FlightOnOffSwitch"
                                Grid.Row="0"
                                Margin="0,40,-9,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                IsHitTestVisible="False"
                                Style="{DynamicResource ModernSwitch}">
                                <CheckBox.Effect>
                                    <DropShadowEffect BlurRadius="20" Opacity="0.6" />
                                </CheckBox.Effect>
                            </CheckBox>

                            <Label
                                Grid.Row="0"
                                Margin="0,82,0,0"
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Top"
                                Content="SIMULATION"
                                FontSize="12"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Border
                                x:Name="SimulationStatusPane"
                                Grid.Row="0"
                                Width="90"
                                Height="28"
                                Margin="0,80,-5,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Background="LimeGreen"
                                CornerRadius="13">
                                <Border.Effect>
                                    <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                </Border.Effect>
                                <Label
                                    x:Name="SimulationStatus"
                                    HorizontalAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    Content="ENABLE"
                                    FontSize="12"
                                    FontWeight="Bold"
                                    Foreground="White" />
                            </Border>

                            <Button
                                x:Name="ImportImageButton"
                                Grid.Row="0"
                                Width="60"
                                Height="60"
                                Margin="10,126,0,15"
                                Click="ImportImageButton_Click"
                                Style="{DynamicResource Export}" />

                            <Border
                                Grid.Row="0"
                                Width="140"
                                Height="30"
                                Margin="15,140,-5,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Background="White"
                                CornerRadius="15">
                                <TextBox
                                    x:Name="csvTextBox"
                                    Width="120"
                                    Height="25"
                                    Margin="0,2,0,2"
                                    Background="White"
                                    BorderThickness="0"
                                    TextWrapping="Wrap" />
                                <Border.Effect>
                                    <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                </Border.Effect>
                            </Border>

                            <!--#endregion-->

                            <!--#region separator-->
                            <Border
                                Grid.Row="1"
                                Width="230"
                                Height="2"
                                Background="White"
                                CornerRadius="2" />
                            <!--#endregion-->

                            <!--#region Serial Port Setting-->

                            <Label
                                Grid.Row="2"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Top"
                                Content="SERIAL PORT"
                                FontSize="20"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Label
                                Grid.Row="2"
                                Margin="0,20,0,0"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Top"
                                Content="SETTING"
                                FontSize="20"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Label
                                Grid.Row="2"
                                Margin="0,73,0,0"
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Top"
                                Content="COMPORT"
                                FontSize="12"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Border
                                Grid.Row="2"
                                Width="125"
                                Height="25"
                                Margin="0,72,-5,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Background="White"
                                CornerRadius="15">
                                <ComboBox
                                    x:Name="ComportDropdown"
                                    Width="100"
                                    Margin="0,2,0,2"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Right"
                                    Background="White"
                                    BorderThickness="0"
                                    FontSize="13"
                                    ItemsSource="{Binding}" />
                            </Border>

                            <Label
                                Grid.Row="2"
                                Margin="0,113,0,0"
                                HorizontalContentAlignment="Left"
                                VerticalContentAlignment="Top"
                                Content="BAUDRATE"
                                FontSize="12"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Border
                                Grid.Row="2"
                                Width="125"
                                Height="25"
                                Margin="0,112,-5,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Background="White"
                                CornerRadius="15">
                                <ComboBox
                                    x:Name="BaudrateDropdown"
                                    Width="100"
                                    Margin="0,2,0,2"
                                    VerticalAlignment="Top"
                                    HorizontalContentAlignment="Right"
                                    Background="White"
                                    BorderThickness="0"
                                    FontSize="13" />
                            </Border>

                            <Grid
                                Grid.Row="2"
                                Width="200"
                                Height="90"
                                Margin="0,140,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Top">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition Width="20" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Button
                                    x:Name="ConnectPortBtn"
                                    Grid.Column="0"
                                    Width="90"
                                    Height="90"
                                    Click="ConnectPortBtn_Click"
                                    Style="{DynamicResource ConnectPort}" />

                                <Button
                                    x:Name="DisconnectPortBtn"
                                    Grid.Column="0"
                                    Width="90"
                                    Height="90"
                                    Click="DisconnectPortBtn_Click"
                                    Style="{DynamicResource DisconnectPort}"
                                    Visibility="Hidden" />

                                <Button
                                    x:Name="RestartPortBtn"
                                    Grid.Column="2"
                                    Width="70"
                                    Height="70"
                                    Margin="10,0,10,0"
                                    Click="RestartPortBtn_Click"
                                    Style="{DynamicResource RestartPort}" />
                            </Grid>

                            <!--#endregion-->

                            <!--#region separator-->
                            <Border
                                Grid.Row="3"
                                Width="230"
                                Height="2"
                                Background="White"
                                CornerRadius="2" />
                            <!--#endregion-->

                            <!--#region MQQT-->
                            <Label
                                Grid.Row="4"
                                HorizontalContentAlignment="Center"
                                VerticalContentAlignment="Top"
                                Content="MQTT"
                                FontSize="20"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Grid
                                Grid.Row="4"
                                Margin="0,40,0,0"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Top">
                                <Button
                                    x:Name="MqttBtn"
                                    Grid.Row="4"
                                    Width="80"
                                    Height="80"
                                    Click="MqttBtn_Click"
                                    Style="{DynamicResource MQQT}" />
                            </Grid>
                            <!--#endregion-->

                        </Grid>
                        <Border.Effect>
                            <DropShadowEffect BlurRadius="20" Opacity="0.6" />
                        </Border.Effect>
                    </Border>

                    <!--#endregion-->

                    <!--#region Icon Settings-->

                    <Border
                        x:Name="SE"
                        Grid.Column="1"
                        Width="40"
                        Height="40"
                        Margin="10,-70,-20,10"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Background="#447695"
                        CornerRadius="27"
                        Visibility="Hidden">
                        <Button
                            x:Name="SidebarEnable"
                            Click="SidebarEnable_Click"
                            Style="{DynamicResource SettingEnable}" />
                        <Border.Effect>
                            <DropShadowEffect BlurRadius="20" Opacity="0.6" />
                        </Border.Effect>
                    </Border>

                    <Border
                        x:Name="SD"
                        Grid.Column="1"
                        Width="40"
                        Height="40"
                        Margin="10,-70,-20,10"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        Background="#447695"
                        CornerRadius="27"
                        Visibility="Hidden">
                        <Button
                            x:Name="SidebarDisable"
                            Click="SidebarDisable_Click"
                            Style="{DynamicResource SettingDisable}" />
                        <Border.Effect>
                            <DropShadowEffect BlurRadius="20" Opacity="0.6" />
                        </Border.Effect>
                    </Border>
                    <!--#endregion-->

                </Grid>

            </Grid>

            <!--#region Screen Filter-->
            <Border
                Name="screenfilter"
                Grid.ColumnSpan="2"
                Width="auto"
                Height="auto"
                Background="Black"
                Opacity="0.8"
                Visibility="Hidden">
                <Border.Effect>
                    <BlurEffect Radius="8" />
                </Border.Effect>
            </Border>
            <!--#endregion-->

            <!--#region Custom Message Bpx-->

            <Border
                Name="CMBox"
                Grid.ColumnSpan="2"
                Width="350"
                Height="200"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Background="#003049"
                CornerRadius="35"
                Visibility="Hidden">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>

                    <Border
                        Grid.Row="1"
                        Margin="5,5,5,5"
                        CornerRadius="35"
                        Style="{DynamicResource DashItemSM}">

                        <Grid>
                            <Border
                                Name="bgmessagebox"
                                Grid.Row="1"
                                CornerRadius="35"
                                Opacity="0.5" />

                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="50" />
                                </Grid.RowDefinitions>

                                <Grid Margin="10,0,0,0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>

                                    <TextBlock
                                        Name="TBContent"
                                        Grid.Row="0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Bottom"
                                        FontSize="25"
                                        Foreground="White"
                                        Text="EXAMPLE" />

                                    <TextBlock
                                        Name="TBContent1"
                                        Grid.Row="1"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        FontSize="15"
                                        Foreground="White"
                                        Text="EXAMPLE" />
                                </Grid>

                                <Grid Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>

                                    <Button
                                        Name="MessageOK"
                                        Grid.Column="0"
                                        Margin="40,5,10,5"
                                        Click="MessageOK_Click"
                                        Style="{DynamicResource RoundedButtonStyle}">
                                        <TextBlock
                                            FontWeight="Bold"
                                            Foreground="White"
                                            Text="YES" />
                                    </Button>

                                    <Button
                                        Name="MessageNO"
                                        Grid.Column="1"
                                        Margin="10,5,40,5"
                                        Click="MessageNO_Click"
                                        Style="{DynamicResource RoundedButtonStyle1}">
                                        <TextBlock
                                            FontWeight="Bold"
                                            Foreground="White"
                                            Text="CANCEL" />
                                    </Button>

                                    <Button
                                        Name="MessageOK1"
                                        Grid.ColumnSpan="2"
                                        Width="100"
                                        Height="40"
                                        Margin="10,5,10,5"
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Click="MessageOK1_Click"
                                        Style="{DynamicResource RoundedButtonStyle}"
                                        Visibility="Hidden">
                                        <TextBlock
                                            FontWeight="Bold"
                                            Foreground="White"
                                            Text="OK" />
                                    </Button>

                                </Grid>

                            </Grid>

                        </Grid>

                    </Border>

                </Grid>
            </Border>

            <!--#endregion-->

        </Grid>
    </Border>
</Window>
