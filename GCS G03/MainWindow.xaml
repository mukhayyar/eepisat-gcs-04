<Window
    x:Class="GCS_G03.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:chart="clr-namespace:Syncfusion.UI.Xaml.Charts;assembly=Syncfusion.SfChart.WPF"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gmapwf="clr-namespace:GMap.NET.WindowsForms;assembly=GMap.NET.WindowsForms"
    xmlns:h="http://helix-toolkit.org/wpf"
    xmlns:local="clr-namespace:GCS_G03"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
    xmlns:zg="clr-namespace:ZedGraph;assembly=ZedGraph"
    xmlns:gmaps="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
    Title="GCS G03"
    Width="1366"
    Height="768"
    Background="Transparent"
    Closed="Window_Closed"
    Loaded="Window_Loaded"
    WindowState="Maximized"
    WindowStyle="None"
    mc:Ignorable="d">
    <Border Style="{DynamicResource AppBg}">
        <Grid>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="260" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <!--#region Navbar-->
            <Grid x:Name="NavPanel" Style="{DynamicResource NavbarBG}">

                <Border
                    Grid.Row="1"
                    Background="#447695"
                    CornerRadius="30">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="200" />
                            <RowDefinition Height="20" />
                            <RowDefinition />
                        </Grid.RowDefinitions>

                        <Image
                            Grid.Row="0"
                            Margin="20,20,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Source="/assets/id.png"
                            Style="{DynamicResource IconMenu}">
                            <Image.Effect>
                                <DropShadowEffect BlurRadius="10" Opacity="0.5" />
                            </Image.Effect>
                        </Image>
                        <Label
                            x:Name="teamIDLabel"
                            Grid.Row="0"
                            Margin="40,22,0,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            Content="1010"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White" />

                        <Image
                            Grid.Row="0"
                            Margin="20,80,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Source="/assets/Time_Menu.png"
                            Style="{DynamicResource IconMenu}">
                            <Image.Effect>
                                <DropShadowEffect BlurRadius="10" Opacity="0.5" />
                            </Image.Effect>
                        </Image>

                        <Label
                            Grid.Row="0"
                            Margin="40,72,0,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            Content="MISSION TIME"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White" />

                        <Label
                            x:Name="missionTimeLabel"
                            Grid.Row="0"
                            Margin="40,92,0,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            Content="00:00:00"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White" />

                        <Image
                            Grid.Row="0"
                            Margin="20,140,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            Source="/assets/icon_softwarestatus.png"
                            Style="{DynamicResource IconMenu}">
                            <Image.Effect>
                                <DropShadowEffect BlurRadius="10" Opacity="0.5" />
                            </Image.Effect>
                        </Image>

                        <Label
                            x:Name="softwareStateLabel"
                            Grid.Row="0"
                            Margin="40,142,0,0"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Top"
                            Content="BOOT"
                            FontSize="15"
                            FontWeight="Bold"
                            Foreground="White" />

                        <!--#region separator-->
                        <Border
                            Grid.Row="1"
                            Width="200"
                            Height="2"
                            Background="White"
                            CornerRadius="2" />
                        <!--#endregion-->

                        <Grid
                            Grid.Row="2"
                            HorizontalAlignment="Center"
                            VerticalAlignment="top">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="50" />
                                <RowDefinition Height="50" />
                                <RowDefinition Height="50" />
                                <RowDefinition Height="50" />
                                <RowDefinition Height="50" />
                                <RowDefinition Height="50" />
                                <RowDefinition Height="50" />
                                <RowDefinition />
                            </Grid.RowDefinitions>

                            <RadioButton
                                x:Name="Homemenu"
                                Grid.Row="0"
                                Click="Homemenu_Click"
                                Style="{DynamicResource Home}" />
                            <RadioButton
                                x:Name="Graphmenu"
                                Grid.Row="1"
                                Click="Graphmenu_Click"
                                Style="{DynamicResource Graph}" />
                            <RadioButton
                                x:Name="Mapmenu"
                                Grid.Row="2"
                                Click="Mapmenu_Click"
                                Style="{DynamicResource Map}" />
                            <RadioButton
                                x:Name="DataCsvmenu"
                                Grid.Row="3"
                                Click="DataCsvmenu_Click"
                                Style="{DynamicResource Datacsv}" />
                            <RadioButton
                                x:Name="ContainerCsvmenu"
                                Grid.Row="4"
                                Click="ContainerCsvmenu_Click"
                                Style="{DynamicResource Containercsv}" />
                            <RadioButton
                                x:Name="PayloadCsv"
                                Grid.Row="5"
                                Click="PayloadCsv_Click"
                                Style="{DynamicResource Payloadcsv}" />
                            <RadioButton
                                x:Name="LoadingModelmenu"
                                Grid.Row="6"
                                Click="LoadingModelmenu_Click"
                                Style="{DynamicResource 3DM}"
                                Visibility="Hidden" />
                        </Grid>

                    </Grid>
                </Border>
            </Grid>
            <!--#endregion-->

            <Grid Grid.Column="1">

                <Grid.RowDefinitions>
                    <RowDefinition Height="80" />
                    <RowDefinition />
                </Grid.RowDefinitions>

                <!--#region TopBar-->
                <Grid x:Name="TopBar" Style="{DynamicResource NavbarBG}">

                    <Border
                        Width="700"
                        Margin="0,3,0,3"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        Background="#447695"
                        CornerRadius="27">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="300" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <Border
                                Grid.Column="0"
                                Background="#4E86A9"
                                CornerRadius="27,0,70,27">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="60" />
                                    </Grid.ColumnDefinitions>

                                    <Image
                                        Grid.Column="0"
                                        Source="/assets/cansat.png"
                                        Style="{DynamicResource Icon}" />
                                    <Image
                                        Grid.Column="1"
                                        Source="/assets/pens.png"
                                        Style="{DynamicResource Icon}" />
                                    <Image
                                        Grid.Column="2"
                                        Width="70"
                                        Height="70"
                                        Margin="0,-5,0,0"
                                        Source="/assets/eepisat-logotype.png"
                                        Style="{DynamicResource Icon}" />

                                </Grid>
                            </Border>

                            <Label
                                Grid.Column="1"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Content="BAMANTARA EEPISAT"
                                FontSize="15"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Border
                                Grid.Column="1"
                                Width="3"
                                Height="15"
                                Margin="167,8,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Top"
                                Background="White" />

                            <Label
                                Grid.Column="1"
                                Margin="0,0,15,0"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Top"
                                Content="GROUND CONTROL STATION"
                                FontSize="15"
                                FontWeight="Bold"
                                Foreground="White" />

                            <Label
                                Grid.Column="1"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Bottom"
                                Content="POLITEKNIK ELEKTRONIKA NEGERI SURABAYA"
                                FontSize="15"
                                FontWeight="Bold"
                                Foreground="White" />

                        </Grid>
                    </Border>

                    <Border
                        Width="150"
                        Height="50"
                        Margin="0,0,120,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Background="#447695"
                        CornerRadius="25">
                        <Grid>
                            <Image
                                Width="42"
                                Height="42"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Left"
                                Source="/assets/logo_indonesia.png"
                                Style="{DynamicResource Icon}" />
                            <Label
                                Margin="48,0,0,0"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                Content="INDONESIA"
                                FontSize="15"
                                FontWeight="Bold"
                                Foreground="White" />
                        </Grid>
                    </Border>

                    <Border
                        Width="50"
                        Height="50"
                        Margin="0,0,60,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Background="#447695"
                        CornerRadius="27">
                        <Button
                            x:Name="RestartBtn"
                            Click="RestartBtn_Click"
                            Style="{DynamicResource Reset}" />
                    </Border>

                    <Border
                        Width="50"
                        Height="50"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Background="#447695"
                        CornerRadius="27">
                        <Button
                            x:Name="ExitBtn"
                            Click="ExitBtn_Click"
                            Style="{DynamicResource Exit}" />
                    </Border>
                </Grid>
                <!--#endregion-->

                <Grid Grid.Row="1">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="250" />
                    </Grid.ColumnDefinitions>

                    <!--#region Main Content-->

                    <TabControl
                        x:Name="MainPage"
                        Grid.Column="0"
                        Width="auto"
                        Height="680"
                        Margin="5,0,20,3"
                        Background="Transparent"
                        BorderBrush="Transparent"
                        ScrollViewer.CanContentScroll="True">

                        <TabControl.ItemContainerStyle>
                            <Style TargetType="{x:Type TabItem}">
                                <Setter Property="Visibility" Value="Collapsed" />
                            </Style>
                        </TabControl.ItemContainerStyle>

                        <TabItem>
                            <!--#region Home-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>

                                <!--#region Bagian 1-->
                                <Grid Grid.Column="0">
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition Height="180" />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>

                                    <Grid Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="210" />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>

                                        <Grid Grid.Column="0">
                                            <Grid.RowDefinitions>
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                            </Grid.RowDefinitions>

                                            <Border
                                                Grid.Row="0"
                                                Margin="6"
                                                CornerRadius="25"
                                                Style="{DynamicResource DashItemSM}">
                                                <Grid>
                                                    <Image
                                                        Margin="20,0,0,0"
                                                        HorizontalAlignment="Left"
                                                        Source="/assets/voltage.png"
                                                        Style="{DynamicResource Icon}" />
                                                    <Label
                                                        x:Name="voltageValue"
                                                        Margin="20,10,0,0"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Top"
                                                        Content="0.00"
                                                        Style="{DynamicResource NavItemLabel}" />
                                                    <Label
                                                        Margin="20,0,0,10"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Bottom"
                                                        Content="VOLT"
                                                        Style="{DynamicResource NavItemLabel}" />
                                                </Grid>
                                            </Border>

                                            <Border
                                                Grid.Row="1"
                                                Margin="6"
                                                CornerRadius="25"
                                                Style="{DynamicResource DashItemSM}">
                                                <Grid>
                                                    <Image
                                                        Margin="10,0,0,0"
                                                        HorizontalAlignment="Left"
                                                        Source="/assets/container.png"
                                                        Style="{DynamicResource Icon}" />
                                                    <Label
                                                        x:Name="containerPacketCountValue"
                                                        Margin="20,10,0,0"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Content="0.00"
                                                        Style="{DynamicResource NavItemLabel}" />
                                                </Grid>
                                            </Border>

                                            <Border
                                                Grid.Row="2"
                                                Margin="6"
                                                CornerRadius="25"
                                                Style="{DynamicResource DashItemSM}">
                                                <Grid>
                                                    <Image
                                                        Margin="23,0,0,0"
                                                        HorizontalAlignment="Left"
                                                        Source="/assets/temperature_menu.png"
                                                        Style="{DynamicResource Icon}" />
                                                    <Label
                                                        x:Name="temperatureValue"
                                                        Margin="20,0,0,0"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Content="0.00"
                                                        Style="{DynamicResource NavItemLabel}" />
                                                    <Label
                                                        Margin="0,0,50,0"
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Content="°C"
                                                        Style="{DynamicResource NavItemLabel}" />
                                                </Grid>
                                            </Border>

                                        </Grid>

                                        <Grid Grid.Column="1">
                                            <Grid.RowDefinitions>
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                            </Grid.RowDefinitions>

                                            <Border
                                                Grid.Row="0"
                                                Grid.RowSpan="2"
                                                Margin="6"
                                                CornerRadius="30"
                                                Style="{DynamicResource DashItemSM}">
                                                <Grid Margin="2,0,-2,0">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="70" />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>

                                                    <Image
                                                        Width="50"
                                                        Height="50"
                                                        Margin="15,0,15,0"
                                                        HorizontalAlignment="Center"
                                                        Source="/assets/maxaltitude.png"
                                                        Style="{DynamicResource IconMenu}" />

                                                    <Grid Grid.Column="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition />
                                                            <RowDefinition Height="10" />
                                                            <RowDefinition />
                                                        </Grid.RowDefinitions>

                                                        <Grid Grid.Row="0">
                                                            <Label
                                                                Margin="0,0,0,20"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Content="MAX ALTITUDE"
                                                                Style="{DynamicResource NavItemLabel}" />
                                                            <Label
                                                                x:Name="altitudeMaxValue"
                                                                Margin="0,20,0,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Content="000.0"
                                                                Style="{DynamicResource NavItemLabel}" />
                                                            <Label
                                                                Margin="60,20,0,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Content="m"
                                                                Style="{DynamicResource NavItemLabel}" />
                                                        </Grid>

                                                        <Border
                                                            Grid.Row="1"
                                                            Margin="10,4,10,4"
                                                            Background="Silver"
                                                            CornerRadius="5" />

                                                        <Grid Grid.Row="2">
                                                            <Label
                                                                Margin="0,0,0,20"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Content="ALTITUDE"
                                                                Style="{DynamicResource NavItemLabel}" />
                                                            <Label
                                                                x:Name="altitudeValue"
                                                                Margin="0,20,0,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Content="000.0"
                                                                Style="{DynamicResource NavItemLabel}" />
                                                            <Label
                                                                Margin="60,20,0,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Content="m"
                                                                Style="{DynamicResource NavItemLabel}" />
                                                        </Grid>

                                                    </Grid>
                                                </Grid>
                                            </Border>

                                            <!-- <Border
                                                Grid.Row="2"
                                                Margin="6"
                                                CornerRadius="25"
                                                Style="{DynamicResource DashItemSM}">
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>
                                                    <Image
                                                        Grid.Column="0"
                                                        Margin="0,0,0,25"
                                                        Source="/assets/icon_logcontainer.png"
                                                        Style="{DynamicResource IconMenu}" />

                                                    <CheckBox
                                                        x:Name="LogDataContainerSwitch"
                                                        Grid.Row="0"
                                                        Margin="0,40,-12,0"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Checked="LogDataContainerSwitch_Checked"
                                                        Style="{DynamicResource ModernSwitch}" />

                                                    <Image
                                                        Grid.Column="1"
                                                        Margin="0,0,0,25"
                                                        Source="/assets/icon_logpayload.png"
                                                        Style="{DynamicResource IconMenu}" />

                                                    <CheckBox
                                                        x:Name="LogDataPayloadSwitch"
                                                        Grid.Row="0"
                                                        Grid.Column="1"
                                                        Margin="0,40,-12,0"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Checked="LogDataPayloadSwitch_Checked"
                                                        Style="{DynamicResource ModernSwitch}" />
                                                </Grid>

                                            </Border>
                                            -->

                                        </Grid>

                                    </Grid>

                                    <Grid Grid.Row="1">
                                        <Border
                                            Grid.Row="0"
                                            Grid.RowSpan="2"
                                            Margin="6"
                                            CornerRadius="30"
                                            Style="{DynamicResource DashItemSM}">
                                            <Grid Margin="2,0,-2,0">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="100" />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>

                                                <Grid>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                    </Grid.RowDefinitions>
                                                    <Image
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Source="/assets/payload.png"
                                                        Style="{DynamicResource Icon}" />
                                                    <Border
                                                        x:Name="payloadSoftwareStateLabelPane"
                                                        Grid.Row="1"
                                                        Margin="5,8,5,8"
                                                        BorderBrush="Silver"
                                                        BorderThickness="2"
                                                        CornerRadius="20"
                                                        Style="{DynamicResource DashItemSM}">
                                                        <Label
                                                            x:Name="payloadSoftwareStateLabel"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="STAND_BY"
                                                            FontSize="8"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                    </Border>

                                                    <Border
                                                        x:Name="PayloadReleasedPane"
                                                        Grid.Row="2"
                                                        Width="30"
                                                        Height="30"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        Background="Firebrick"
                                                        CornerRadius="7">
                                                        <Label
                                                            x:Name="PayloadReleased"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="N"
                                                            FontSize="15"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                    </Border>

                                                </Grid>

                                                <Grid Grid.Column="1">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition />
                                                        <ColumnDefinition Width="10" />
                                                        <ColumnDefinition />
                                                    </Grid.ColumnDefinitions>

                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition Height="10" />
                                                        <RowDefinition />
                                                    </Grid.RowDefinitions>

                                                    <Grid Grid.Column="0">
                                                        <Label
                                                            Margin="0,0,0,20"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="PACKET COUNT"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            x:Name="PayloadPacketCountValue"
                                                            Margin="0,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="000"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                    </Grid>

                                                    <Grid Grid.Column="2">
                                                        <Label
                                                            Margin="0,0,0,20"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="ALTITUDE"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            x:Name="PayloadAltitudeValue"
                                                            Margin="0,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="000.0"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            Margin="60,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="m"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                    </Grid>

                                                    <!--#region separator-->
                                                    <Border
                                                        Grid.Row="0"
                                                        Grid.RowSpan="3"
                                                        Grid.Column="1"
                                                        Margin="4,10,4,10"
                                                        Background="Silver"
                                                        CornerRadius="5" />
                                                    <Border
                                                        Grid.Row="1"
                                                        Grid.Column="0"
                                                        Grid.ColumnSpan="3"
                                                        Margin="10,4,10,4"
                                                        Background="Silver"
                                                        CornerRadius="5" />
                                                    <!--#endregion-->

                                                    <Grid Grid.Row="2" Grid.Column="0">
                                                        <Label
                                                            Margin="0,0,0,20"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="TEMPERATURE"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            x:Name="PayloadTemperatureValue"
                                                            Margin="0,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="00.0"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            Margin="60,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="°C"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                    </Grid>

                                                    <Grid Grid.Row="2" Grid.Column="2">
                                                        <Label
                                                            Margin="0,0,0,20"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="VOLTAGE"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            x:Name="PayloadVoltageValue"
                                                            Margin="0,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="00.0"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            Margin="70,20,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="Volt"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                    </Grid>

                                                </Grid>

                                            </Grid>
                                        </Border>
                                    </Grid>

                                    <Grid Grid.Row="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition />
                                            <ColumnDefinition />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>

                                        <!--#region accelerometer-->
                                        <Border
                                            Grid.Row="0"
                                            Margin="6"
                                            CornerRadius="30"
                                            Style="{DynamicResource DashItemSM}">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="70" />
                                                    <ColumnDefinition />
                                                </Grid.ColumnDefinitions>

                                                <Image
                                                    Grid.Column="0"
                                                    Margin="0,0,0,60"
                                                    Source="/assets/accel.png"
                                                    Style="{DynamicResource IconMenu}" />

                                                <Grid Grid.Column="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition Height="25" />
                                                        <RowDefinition Height="25" />
                                                        <RowDefinition Height="25" />
                                                    </Grid.RowDefinitions>

                                                    <Label
                                                        Margin="0,5,0,5"
                                                        HorizontalAlignment="Left"
                                                        VerticalAlignment="Center"
                                                        Content="ACCELEROMETER"
                                                        Style="{DynamicResource NavItemLabel}" />

                                                    <Grid Grid.Row="1" Margin="0,0,5,0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="20" />
                                                            <ColumnDefinition />
                                                            <ColumnDefinition Width="40" />
                                                        </Grid.ColumnDefinitions>
                                                        <Label
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
                                                            Content="R"
                                                            Style="{DynamicResource NavItemLabel}" />
                                                        <Label
                                                            x:Name="ACCEL_RValue"
                                                            Grid.Column="1"
                                                            Margin="0,0,0,0"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center"
