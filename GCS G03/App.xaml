            <Style x:Key="3DM" TargetType="{x:Type RadioButton}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Margin" Value="2,2,-7,2" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type RadioButton}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="22,0,0,22">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="75" />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="30" />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Source="/assets/3DM_Menu.png"
                                        Style="{DynamicResource IconItemList}">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
