                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Source="/assets/mount_map.png"
                                        Style="{DynamicResource IconMenu}">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                    </Image>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
