<Application
    x:Class="GCS_G03.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Light.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.DeepPurple.xaml" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Accent/MaterialDesignColor.Lime.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <!--#region Basic Style-->

            <Style x:Key="AppBg" TargetType="Border">
                <Setter Property="Background" Value="#003049" />
                <Setter Property="CornerRadius" Value="30" />
            </Style>

            <Style x:Key="DashItemSM" TargetType="Border">
                <Setter Property="Background" Value="#447695" />
                <Setter Property="CornerRadius" Value="40" />
                <Setter Property="Margin" Value="20,20,20,20" />
            </Style>

            <Style x:Key="DashItemMD" TargetType="Border">
                <Setter Property="Background" Value="#447695" />
                <Setter Property="CornerRadius" Value="50" />
                <Setter Property="Margin" Value="20,40,20,40" />
            </Style>

            <Style x:Key="DashItemLG" TargetType="Border">
                <Setter Property="Background" Value="#447695" />
                <Setter Property="CornerRadius" Value="50" />
                <Setter Property="Margin" Value="20,40,20,40" />
            </Style>

            <Style x:Key="DashIcon" TargetType="Border">
                <Setter Property="Background" Value="#447695" />
                <Setter Property="CornerRadius" Value="50" />
                <Setter Property="Margin" Value="20,40,20,40" />
            </Style>

            <Style x:Key="NavbarBG" TargetType="Grid">
                <Setter Property="Margin" Value="10" />
            </Style>

            <Style x:Key="SettingMenu" TargetType="Border">
                <Setter Property="Background" Value="#447695" />
                <Setter Property="CornerRadius" Value="50" />
            </Style>

            <Style x:Key="FloatLogo" TargetType="Border">
                <Setter Property="Background" Value="White" />
                <Setter Property="CornerRadius" Value="30,0,0,0" />
            </Style>

            <Style x:Key="TopBarMenu" TargetType="Border">
                <Setter Property="CornerRadius" Value="30" />
                <Setter Property="Background" Value="#528FB5" />
                <Setter Property="Margin" Value="15" />
            </Style>

            <Style x:Key="NavItemLabel" TargetType="Label">
                <Setter Property="Foreground" Value="White" />
                <Setter Property="FontWeight" Value="Bold" />
            </Style>

            <Style x:Key="NavMenuLabel" TargetType="Label">
                <Setter Property="Foreground" Value="White" />
                <Setter Property="FontWeight" Value="Bold" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="VerticalAlignment" Value="Center" />
            </Style>
            <!--#endregion-->

            <!--#region Icon Style-->

            <Style x:Key="Icon" TargetType="Image">
                <Setter Property="Width" Value="50" />
                <Setter Property="Height" Value="50" />
                <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
                <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />
            </Style>

            <Style x:Key="IconMenu" TargetType="Image">
                <Setter Property="Width" Value="40" />
                <Setter Property="Height" Value="40" />
                <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
                <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />
            </Style>

            <Style x:Key="IconItemList" TargetType="Image">
                <Setter Property="Width" Value="25" />
                <Setter Property="Height" Value="25" />
                <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
                <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />
            </Style>

            <!--#endregion-->

            <!--#region Button App Style-->

            <Style x:Key="BtnImg" TargetType="Button">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Width" Value="50" />
                <Setter Property="Height" Value="50" />
                <Setter Property="HorizontalAlignment" Value="Center" />
                <Setter Property="VerticalAlignment" Value="Center" />
            </Style>

            <Style x:Key="BtnIcon" TargetType="Image">
                <Setter Property="RenderOptions.BitmapScalingMode" Value="HighQuality" />
                <Setter Property="RenderOptions.EdgeMode" Value="Aliased" />
            </Style>

            <Style x:Key="Exit" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="35">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Source="/assets/power2.png"
                                        Style="{DynamicResource IconMenu}">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                    </Image>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="Darkred" />
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True">
                                    <Setter Property="Background" Value="Red" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="Reset" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="35">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Source="/assets/Restart.png"
                                        Style="{DynamicResource IconMenu}">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                    </Image>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="DarkCyan" />
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True">
                                    <Setter Property="Background" Value="Cyan" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="SettingEnable" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="35">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Width="35"
                                        Height="35"
                                        Source="/assets/Left.png"
                                        Style="{DynamicResource IconMenu}">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                    </Image>
                                </Grid>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="SettingDisable" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="35">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Width="35"
                                        Height="35"
                                        Source="/assets/right.png"
                                        Style="{DynamicResource IconMenu}">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                    </Image>
                                </Grid>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <!--#region ConnectPort Button-->
            <Style x:Key="ConnectPort" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Margin="10"
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="35">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        Grid.Column="0"
                                        Width="60"
                                        Height="60">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                        <Image.Style>
                                            <Style TargetType="{x:Type Image}">
                                                <Setter Property="Source" Value="/Assets/icon_Disconnect.png" />
                                                <Style.Triggers>

                                                    <Trigger Property="IsMouseOver" Value="True" />
                                                </Style.Triggers>
                                            </Style>
                                        </Image.Style>
                                    </Image>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="#33586f" />
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True">
                                    <Setter Property="Background" Value="#558bac" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <!--#endregion-->

            <!--#region DisconnectPort Button-->
            <Style x:Key="DisconnectPort" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Left" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border
                                Margin="10"
                                Background="{TemplateBinding Background}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="35">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Image
                                        x:Name="DisconnectPortImg"
                                        Grid.Column="0"
                                        Width="60"
                                        Height="60">
                                        <Image.Effect>
                                            <DropShadowEffect BlurRadius="8" Opacity="0.2" />
                                        </Image.Effect>
                                        <Image.Style>
                                            <Style TargetType="{x:Type Image}">
                                                <Setter Property="Source" Value="/Assets/icon_Connect.png" />
                                                <Style.Triggers>

                                                    <Trigger Property="IsMouseOver" Value="True" />
                                                </Style.Triggers>
                                            </Style>
                                        </Image.Style>
                                    </Image>
                                </Grid>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="#33586f" />
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True">
                                    <Setter Property="Background" Value="#558bac" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <!--#endregion-->

            <!--#region RestartPort Button-->
            <Style x:Key="RestartPort" TargetType="{x:Type Button}">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="HorizontalAlignment" Value="Left" />
